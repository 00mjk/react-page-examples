{
  "name": "react",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "upgrade-react-page": "export PACKAGE=''; for i in editor plugins-background plugins-slate plugins-image plugins-spacer; do PACKAGE=\"${PACKAGE} @react-page/${i}@latest\"; done;  yarn add ${PACKAGE}"
  },
  "dependencies": {
    "@react-page/editor": "^0.27.2",
    "@react-page/plugins-background": "^0.27.2",
    "@react-page/plugins-slate": "^0.27.2",
    "@react-page/plugins-image": "^0.27.2",
    "@react-page/plugins-spacer": "^0.27.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "typescript": "^4.0.3"
  },
  "devDependencies": {
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-react": "^7.21.2",
    "prettier": "^2.1.2",
    "prettier-eslint": "^11.0.0",
    "react-scripts-ts": "latest"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
